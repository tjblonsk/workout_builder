<h1>All Videos</h1>

<% @videos.each do |video| %>
<ul>
  <li><%= link_to video.name, video.url, :target => '_blank' %>
      (<%= video.lift_id %>)
  <form action="/lifts/<%= video.id %>" method="post">
    <input type="hidden" name="_method" value="delete">
    <button>DELETE VIDEO</button>
  </form>
  </li>
</ul>
  <% end %>