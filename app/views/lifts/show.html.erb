<h1><%= @lift.name %></h1>

<ul>
  <% @videos.each do |video| %>
  <li><a href="<%= video.url %>" target="_blank"><%= video.name %></a>
      <ul>
        <li><%= video.description %></li>
        <li>Date Added: <%= video.created_at %></li>
      </ul>
<%= button_to "delete", {:controller => :lifts,
      :action => 'destroy', :id => video.id },     :method => :delete %>
  </li>
  <% end %>
</ul>



